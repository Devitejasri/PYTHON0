a=int(input())
print('a<<1:',a<<1)
print('a<<2:',a<<2)
print('a<<3:',a<<3)
print('a<<4:',a<<4)
print('a<<5:',a<<5)
print('a<<6:',a<<6)
print('a<<7:',a<<7)
print('a<<8:',a<<8)
print('a<<9:',a<<9)
print('a<<10:',a<<10)

OUTPUT
10
a<<1: 20
a<<2: 40
a<<3: 80
a<<4: 160
a<<5: 320
a<<6: 640
a<<7: 1280
a<<8: 2560
a<<9: 5120
a<<10: 10240
